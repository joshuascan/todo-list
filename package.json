{
  "name": "todo-list",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "migrateh": "heroku run knex migrate:latest -a forgetful-js",
    "rollbackh": "heroku run knex migrate:rollback -a forgetful-js",
    "databaseh": "heroku pg:psql -a forgetful-js",
    "seedh": "heroku run knex seed:run -a forgetful-js",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "14.18.0",
    "npm": "6.14.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuascan/todo-list.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshuascan/todo-list/issues"
  },
  "homepage": "https://github.com/joshuascan/todo-list#readme",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "knex-cleaner": "^1.3.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "nodemon": "^2.0.13"
  }
}
